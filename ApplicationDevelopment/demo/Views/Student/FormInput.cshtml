
<form action="FormOutput" method="get">
    <div class="form-group">
        <label class="col-form-label">Enter name: </label>
        <input type="text" class="form-control" name="Name"/>
    </div>    
       <div class="form-group">
        <label class="col-form-label">Enter age: </label>
        <input type="number" class="form-control" name="Age"/>
    </div>   
    <div class="form-group">
        <label class="form-label">Enter gender: </label>
        <input type="radio" value="Male" name="Gender" /> Male
        <input type="radio" value="Female" name="Gender" /> Female
    </div>
    <div class="form-group">
        <input type="checkbox" name="IsGraduated" value="true"/> Is Graduated
    </div>
    <input type="submit" value="Create" />
</form>